{
  "env": { "es6": true, "browser": true },
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "semi": [2, "never"], # 不允许分号
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }], # js 扩展名
    "react/jsx-first-prop-new-line": 0, # 属性不需另起一行
    "jsx-a11y/no-static-element-interactions": 0,

    # "comma-dangle": 0,
    # "no-console": 0,
    # "no-alert": 0,
    # "id-length": 0,

    # "no-param-reassign": [2, { "props": false }], # 参数不可修改，属性可修改
    # "no-underscore-dangle": 0,                    # 允许 _
    # "global-require": 0,
    # "no-class-assign": 0,
    # "react/prop-types": 0,
    # "react/prefer-stateless-function": 0,
    # "react/jsx-no-bind": 0
  }
}
